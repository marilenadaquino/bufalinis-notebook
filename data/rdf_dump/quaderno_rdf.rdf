   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/citation/g-g-belli-sonetti-romaneschi-la-verita">
      <rdf:type rdf:resource="http://purl.org/spar/cito/Citation"/>
      <cito:hasCitingEntity rdf:resource="https://w3id.org/bufalinis-notebook/page/100"/>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl167">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">La verità è comm'è la cacarella</l>
                     <l part="N">che cquanno te viè l'impito e te scappa</l>
                     <l part="N">hai tempo, fija, de serrà la chiappa</l>
                     <l part="N">e storcete e tremà pe' trattenella.</l>
                  </lg>
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">E accusì si la bocca nun z'attappa,</l>
                     <l part="N">La santa Verità sbrodorarella</l>
                     <l part="N">T'esce fora da sé da le budella</l>
                     <l part="N">Fossi tu puro un frate de la Trappa</l>
                  </lg>
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">Perché scha da stà zitti, o<del rend="overstrike" instant="false" status="unremarkable">
                           <gap reason="deleted" instant="false"/>
                        </del>ddi una miffa</l>
                     <l part="N">ogni<del rend="overstrike" instant="false" status="unremarkable">q</del>cuarvorta so le cose vere?</l>
                  </lg>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/100"/>
         </rdf:Description>
      </cito:hasCitedEntity>
