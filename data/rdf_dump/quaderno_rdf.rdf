      <frbr:translationOf rdf:resource="https://w3id.org/bufalinis-notebook/quote/quot-130"/>
   </rdf:Description>
   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/comment/tra-050a">
      <rdf:type rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
      <oa:hasTarget rdf:resource="https://w3id.org/bufalinis-notebook/citation/g-flaubert-m-bovary"/>
      <oa:hasBody>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/note/tra-050a">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0"
                        type="translation"
                        xml:id="tra-050a"
                        prev="#tra-050"
                        corresp="#quot-131"
                        resp="#PB"
                        anchored="true">
                  <p part="N">La morte di qualcuno suscita sempre un senso <lb/>di stupefazione, tanto <add place="above" instant="false" status="unremarkable">è</add> difficile capire questo <lb/>sopraggiungere del nulla, e rassegnarsi a credervi.</p>
               </section>
            </rdf:value>
            <frbr:translationOf rdf:resource="https://w3id.org/bufalinis-notebook/quote/quot-131"/>
            <frbr:realizer rdf:resource="https://w3id.org/bufalinis-notebook/person/pb"/>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/94"/>
         </rdf:Description>
      </oa:hasBody>
