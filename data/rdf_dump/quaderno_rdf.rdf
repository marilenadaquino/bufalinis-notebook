   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/citation/am-inni-sacri-il-nome-di-maria">
      <rdf:type rdf:resource="http://purl.org/spar/cito/Citation"/>
      <cito:hasCitingEntity rdf:resource="https://w3id.org/bufalinis-notebook/page/4"/>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl004">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l part="N">in reverenti</l>
                  <l part="N">accoglienze onorò l'inaspettata</l>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/4"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl004a">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">Noi sappiamo, o Maria, ch'Ei solo attenne</l>
                     <l part="N">l'alta promessa che da Te s'udìa,</l>
                     <l part="N">Ei che in cor la ti pose: a noi solenne</l>
                     <l part="N">è il nome tuo, Maria.</l>
                  </lg>
                  <ab type="ellipsis" part="N">....</ab>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/4"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl004b">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l part="N">Salve, beata: in quale età scortese</l>
                  <l part="N">quel sì caro a ridir nome si tacque?</l>
                  <ab type="ellipsis" part="N">....</ab>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/4"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl004c">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l part="N">Quai monti mai, quali acque</l>
                  <l part="N">non l'udiro invocar? La terra antica</l>
                  <pb n="5" facs="quad005r"/>
                  <l part="N">non porta sola i templi tuoi, ma quella</l>
                  <l part="N">che il Genovese divinò, nutrica</l>
                  <l part="N">i tuoi cultori anch'ella.</l>
                  <ab type="ellipsis" part="N">....</ab>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/4"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl004e">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">Te, quando sorge, e quando cade il die,</l>
                     <l part="N">e quando il sole a mezzo corso il parte,</l>
                     <l part="N">saluta il bronzo che le parole pie</l>
                     <l part="N">invita ad onorarte.</l>
                  </lg>
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">Nelle paure della veglia bruna</l>
                     <l part="N">te noma il fanciullino; a Te tremante</l>
                     <l part="N">quando ingrossa ruggendo la fortuna,</l>
                     <l part="N">ricorre il navigante.</l>
                  </lg>
                  <ab type="ellipsis" part="N">....</ab>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/4"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl004f">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l rend="ast_left_circled" part="N">
                     <hi rend="underline">Tu pur, beata, un dì provasti il pianto</hi>
                  </l>
                  <ab type="ellipsis" part="N">...</ab>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/4"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl004g">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l rend="underline" part="N">tanto piacque al Signor di porre in cima</l>
                  <l rend="underline" part="N">questa fanciulla ebrea</l>
                  <addSpan place="below"
                           spanTo="#a002"
                           instant="false"
                           status="unremarkable"/>
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">O prole d'Israello, o nell'estremo <del rend="overstrike" instant="false" status="unremarkable">caduta</del>
                     </l>
                     <l part="N">caduta, o da sì lunga ira contrita,</l>
                     <l part="N">non è Costei che in onor tanto avemo</l>
                     <l part="N">di vostra gente uscita?</l>
                  </lg>
                  <anchor xml:id="a002"/>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/4"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl004h">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l part="N">O degli afflitti scampo,</l>
                  <l part="N">
                     <ref xml:id="quot-005h-1" target="#quot-007-1" rend="underline">inclita</ref> come il sol, terribil come</l>
                  <l part="N">oste schierata in campo.</l>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/4"/>
         </rdf:Description>
      </cito:hasCitedEntity>
   </rdf:Description>
