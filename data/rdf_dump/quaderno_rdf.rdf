   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/citation/ceronetti-translation-marziale-1-88">
      <rdf:type rdf:resource="http://purl.org/spar/cito/Citation"/>
      <cito:hasCitingEntity rdf:resource="https://w3id.org/bufalinis-notebook/page/63"/>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl089">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">Tu, che nel fiore degli anni al tuo signor</l>
                     <l part="N">rapito fosti, Alcimo, e sulla via</l>
                     <l part="N">labicana ora d'erba leggera</l>
                     <l part="N">un velo copre, da me non avrai</l>
                     <l part="N">un traballante masso di pario,</l>
                     <l part="N">fatica, per poca cenere, vana</l>
                     <l part="N">e col seme della tua distruzione, ma</l>
                     <l part="N">ma bossi spezzati, larghe ombre di pampini</l>
                     <l part="N">e l'erba che fa verde la rugiada</l>
                     <l part="N">delle mie lacrime, fanciullo caro,</l>
                     <l part="N">per testimoni del mio dolore:</l>
                     <l part="N">questo tributo non avrà mai fine.</l>
                     <l part="N">Quando sarà Lachesis al punto</l>
                     <l part="N">estremo della mia vita giunta,</l>
                     <l part="N">così anche le ceneri mie posino.</l>
                  </lg>
               </section>
            </rdf:value>
            <dcterms:relation rdf:resource="https://w3id.org/bufalinis-notebook/quote/quot-096"/>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/63"/>
         </rdf:Description>
      </cito:hasCitedEntity>
   </rdf:Description>
