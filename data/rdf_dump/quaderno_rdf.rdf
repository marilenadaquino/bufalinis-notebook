         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl161c">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <p part="N">pravvivere.</p>
               </section>
            </rdf:value>
            <dcterms:relation rdf:resource="https://w3id.org/bufalinis-notebook/note/comm-134"/>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/136"/>
         </rdf:Description>
