         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl019">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <ab type="ellipsis" part="N">...</ab>
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">In che lande selvagge, in quali mari</l>
                     <l part="N">Di sì barbaro nome fior si coglie,</l>
                     <l part="N">Che non conosca dei tuoi miti altari</l>
                     <l part="N">Le benedette soglie?</l>
                  </lg>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/99"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl019a">
