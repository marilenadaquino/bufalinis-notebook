   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/citation/virgilio-eneide-7-5-9">
      <rdf:type rdf:resource="http://purl.org/spar/cito/Citation"/>
      <cito:hasCitingEntity rdf:resource="https://w3id.org/bufalinis-notebook/page/8"/>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl016">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">At pius exequiis Aeneas rite solutis,</l>
                     <l part="N">aggere composito tumuli, postquam alta quierunt</l>
                     <l part="N">aequora, tendit iter velis portumque relinquit.</l>
                     <l part="N">Adspirant aurae in noctem nec candida cursus</l>
                     <l part="N">luna negat, splendet tremulo sub lumine pontus.</l>
                  </lg>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/8"/>
