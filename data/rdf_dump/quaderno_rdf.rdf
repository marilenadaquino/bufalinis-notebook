   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/citation/tm-la-montagna-incantata-1">
      <rdf:type rdf:resource="http://purl.org/spar/cito/Citation"/>
      <cito:hasCitingEntity rdf:resource="https://w3id.org/bufalinis-notebook/page/1"/>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl063b">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <p part="N">L'uomo sveglio è pur sempre migliore <lb/>di quello che sogna.</p>
                  <p part="N">E G. C. tornò a scorgere quel sorriso... a sua <lb/>vergogna... e con improvviso ritorno alla ragione.</p>
                  <p part="N">Ma ci si abitua a non abituarsi.</p>
                  <p part="N">Il riso non voleva uscire dalla fine ed arida <lb/>tensione dell'angolo della sua bocca.</p>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/1"/>
         </rdf:Description>
      </cito:hasCitedEntity>
   </rdf:Description>
