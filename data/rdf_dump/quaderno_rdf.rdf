   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/citation/targa-ludovicae-vistarini">
      <rdf:type rdf:resource="http://purl.org/spar/cito/Citation"/>
      <cito:hasCitingEntity rdf:resource="https://w3id.org/bufalinis-notebook/page/17"/>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/targa-vistarini">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">Ave filia carissima</l>
                     <l part="N">cui iam anilis prudentia matronalis gravitas</l>
                     <l part="N">cum suavitate puellari et virginali verecundia</l>
                     <l part="N">sunt comitatae.</l>
                  </lg>
                  <milestone unit="section" rend="short_double_hrule"/>
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">Memoriae et quieti</l>
                     <l part="N">Ludovicae Vistarini</l>
                     <l part="N">domo Romae</l>
                     <l part="N">quae morum suavitate musicis modis</l>
                     <l part="N">formae praestantia parentibus gaudio erat et spei</l>
                     <l part="N">annum agens <del rend="overstrike" instant="false" status="unremarkable">sex</del>XVI</l>
                     <l part="N">rapido mortis impetu decessit</l>
                     <l part="N">Die VII Kal. iuniis A.D. MCCCXX.</l>
                  </lg>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/17"/>
         </rdf:Description>
      </cito:hasCitedEntity>
   </rdf:Description>
