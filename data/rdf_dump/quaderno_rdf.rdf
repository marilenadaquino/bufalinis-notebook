         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl060e">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l part="N">Snella per tutto il mondo e lieta vola</l>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/108"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl060f">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">Breve è verso l'eterno ogni altro tempo,</l>
                     <l part="N">e nulla è più legger d'un grato peso.</l>
                  </lg>
