   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/citation/marziale-epigrammi-5-34">
      <rdf:type rdf:resource="http://purl.org/spar/cito/Citation"/>
      <cito:hasCitingEntity rdf:resource="https://w3id.org/bufalinis-notebook/page/52"/>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl081">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">Hanc tibi, Fronto pater, genetrix Faccilla, puellam</l>
                     <l part="N">oscula commendo deliciasque meas,</l>
                     <l part="N">parvola ne nigras horrescat Erotion umbras</l>
                     <l part="N">oraque Tartarei prodigiosa canis.</l>
                     <l part="N">Impletura fuit sextae modo frigora brumae,</l>
                     <l part="N">vixisset totidem ni minus illa dies.</l>
                     <l part="N">Inter tam veteres ludat lasciva patronos</l>
                     <l part="N">et nomen blaeso garriat ore meum.</l>
                     <l part="N">Mollia non rigidus caespes tegat ossa nec illi,</l>
                     <l part="N">terra, gravis fueris: non fuit illa tibi.</l>
                  </lg>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/52"/>
         </rdf:Description>
      </cito:hasCitedEntity>
   </rdf:Description>
