   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/comment/comm-122">
      <rdf:type rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
      <oa:hasTarget rdf:resource="https://w3id.org/bufalinis-notebook/citation/lucrezio-de-rerum-natura"/>
