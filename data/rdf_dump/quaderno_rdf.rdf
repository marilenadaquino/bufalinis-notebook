   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/comment/tra-009">
      <rdf:type rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
      <oa:hasTarget rdf:resource="https://w3id.org/bufalinis-notebook/citation/"/>
      <oa:hasBody>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/note/tra-009">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0"
                        type="translation"
                        xml:id="tra-009"
                        corresp="#quot-031"
                        resp="#PB"
                        anchored="true">
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">Anche colui che grandi imprese di guerra</l>
                     <l part="N">gloriosamente compì,</l>
                     <l part="N">e le cui gesta ancor vive e valide sono,</l>
                     <l part="N">e che più d'ogni altro agli occhi dei popoli s'innalza,</l>
                     <l part="N">- questo, suo padre una volta strappò via</l>
                     <l part="N">dall'amica <del rend="overstrike" instant="false" status="unremarkable">appena coperto di una</del>
                        <add place="above" instant="false" status="unremarkable">in</add> camicia.</l>
                  </lg>
               </section>
            </rdf:value>
            <frbr:translationOf rdf:resource="https://w3id.org/bufalinis-notebook/quote/quot-031"/>
            <frbr:realizer rdf:resource="https://w3id.org/bufalinis-notebook/person/pb"/>
