   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/citation/horatio-carmina-2-3">
      <rdf:type rdf:resource="http://purl.org/spar/cito/Citation"/>
      <cito:hasCitingEntity rdf:resource="https://w3id.org/bufalinis-notebook/page/134"/>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl158">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">Omnes eodem cogimur, omnium</l>
                     <l part="N">versatur urna serius ocius</l>
                     <l part="N">Sors exitura et nos in aeternum</l>
                     <l part="N">exilium impositura cumbae.</l>
