                     <l part="N">del <del rend="overstrike" instant="false" status="unremarkable">nostro</del>
                        <add place="above" instant="false" status="unremarkable">mio</add> dolore. <del rend="overstrike" instant="false" status="unremarkable">In</del>
                        <del rend="overwrite" instant="false" status="unremarkable">p</del>
                        <add rend="superimpose" instant="false" status="unremarkable">P</add>erpetuo vivrà</l>
                     <l part="N">
                        <del rend="overstrike" instant="false" status="unremarkable">vivrà</del> per te questo <del rend="overstrike" instant="false" status="unremarkable">nostro</del>
                        <add place="above" instant="false" status="unremarkable">nostro</add> tributo. E quando</l>
                     <l part="N">
                        <del rend="overstrike" instant="false" status="unremarkable">E quando,</del> anche per me Làchesi sarà giunta</l>
                     <l part="N">
                        <del rend="overstrike" instant="false" status="unremarkable">al punto estremo degli anni, io dispongo,</del>
                        <add place="above" instant="false" status="unremarkable">all'anno estremo, non diversamente <del rend="overstrike" instant="false" status="unremarkable">,</del>
                        </add>
                     </l>
                     <l part="N">
                        <del rend="overstrike" instant="false" status="unremarkable">Làchesi sarà giunta,</del>
