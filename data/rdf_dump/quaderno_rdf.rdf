   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/comment/tra-054">
      <rdf:type rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
      <oa:hasTarget rdf:resource="https://w3id.org/bufalinis-notebook/citation/horatio-ars-poetica"/>
      <oa:hasBody>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/note/tra-054">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0"
                        type="translation"
                        xml:id="tra-054"
                        corresp="#quot-155"
                        resp="#PB"
                        anchored="true">
                  <l part="N">Mi studio di esser breve, divento oscuro;</l>
                  <l part="N">se uno tiene dietro ad uno stile semplice, gli vengon meno</l>
                  <l part="N">il nerbo e il sentimento;</l>
                  <l part="N">chi professa il grande, si gonfia;</l>
                  <l part="N">striscia a terra chi troppo è cauto e teme la tempesta:</l>
                  <l part="N">fuggire un difetto porta a quello opposto, se manca l'arte.</l>
               </section>
            </rdf:value>
            <frbr:translationOf rdf:resource="https://w3id.org/bufalinis-notebook/quote/quot-155"/>
            <frbr:realizer rdf:resource="https://w3id.org/bufalinis-notebook/person/pb"/>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/115"/>
         </rdf:Description>
      </oa:hasBody>
      <oa:motivatedBy>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/translation">
            <rdfs:label>translation</rdfs:label>
         </rdf:Description>
      </oa:motivatedBy>
      <frbr:translationOf rdf:resource="https://w3id.org/bufalinis-notebook/quote/quot-155"/>
   </rdf:Description>
   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/comment/tra-055">
      <rdf:type rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
      <oa:hasTarget rdf:resource="https://w3id.org/bufalinis-notebook/citation/horatio-ars-poetica"/>
      <oa:hasBody>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/note/tra-055">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0"
                        type="translation"
                        xml:id="tra-055"
                        corresp="#quot-156"
                        resp="#PB"
                        anchored="true">
                  <note xml:id="tra-055-1"
                        type="translation"
                        corresp="#quot-156-1"
                        n="(1)"
                        anchored="true">
                     <l part="N">Né facundia gli mancherà né lucid'ordine</l>
                  </note>
                  <note xml:id="tra-055-2"
                        type="translation"
                        corresp="#quot-156-2"
                        n="(2)"
                        anchored="true">
                     <l part="N">Dell'ordine questa sarà la virtù e la bellezza (o io m'inganno)</l>
                     <l part="N">che già fin da ora dica le cose che fin da ora si devono dire</l>
                  </note>
                  <note xml:id="tra-055-3"
                        type="translation"
                        corresp="#quot-156b-1"
                        n="(3)"
                        anchored="true">
                     <l part="N">Se una ingegnosa unione abbia reso nuova una parola nota</l>
                  </note>
               </section>
            </rdf:value>
            <frbr:translationOf rdf:resource="https://w3id.org/bufalinis-notebook/quote/quot-156"/>
            <frbr:realizer rdf:resource="https://w3id.org/bufalinis-notebook/person/pb"/>
