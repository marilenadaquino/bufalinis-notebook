   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/citation/g-g-belli-sonetti-805">
      <rdf:type rdf:resource="http://purl.org/spar/cito/Citation"/>
      <cito:hasCitingEntity rdf:resource="https://w3id.org/bufalinis-notebook/page/134"/>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl159">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <title>Er caffettiere filosofo.</title>
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">L'ommini de sto monno so' l'istesso</l>
                     <l part="N">che vaghi de caffè <del rend="overstrike" instant="false" status="unremarkable">nel</del> ner macinino;</l>
                     <l part="N">C'uno primo, uno dopo e un artro appresso</l>
                     <l part="N">tutti (quanti) <add place="above" instant="false" status="unremarkable">cuanti</add> però vanno a un distino</l>
                     <l part="N">Spesso muteno sito, e caccia spesso</l>
                     <l part="N">er vago grosso er vago piccinino, </l>
                     <l part="N">e s'incarzeno tutti in su l'ingresso</l>
                     <l part="N">der ferro che li sfragne in porverino</l>
                     <l part="N">E l'ommini accusì viveno ar monno</l>
                     <l part="N">
                        <ref xml:id="quot-185-1" target="#comm-129">misticati <add place="above" instant="false" status="unremarkable">(I)</add>
                        </ref> pe mano de la <del rend="overstrike" instant="false" status="unremarkable">
                           <gap reason="deleted" instant="false"/>
                        </del> sorte</l>
                     <l part="N">che si li gira tutti <del rend="overstrike" instant="false" status="unremarkable">intorno intorno</del> in tonno in tonno;</l>
                     <l part="N">E movennose ognuno, o piano o forte, </l>
                     <l part="N">senza capillo mai <del rend="overstrike" instant="false" status="unremarkable">
                           <gap reason="deleted" instant="false"/>
                        </del> caleno a fonno</l>
                     <l part="N">pe' cascà ne la gola de la morte.</l>
                  </lg>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/134"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl159a">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <p part="N">L'ommini... tutti vanno a un distino.</p>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/134"/>
