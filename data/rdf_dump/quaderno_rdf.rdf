   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/citation/am-frammenti-ognissanti">
      <rdf:type rdf:resource="http://purl.org/spar/cito/Citation"/>
      <cito:hasCitingEntity rdf:resource="https://w3id.org/bufalinis-notebook/page/111"/>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl123">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">A Quello domanda, o sdegnoso,</l>
                     <l part="N">perché sull'inospite piagge,</l>
                     <l part="N">all'alito d'aure selvagge,</l>
                     <l part="N">fa sorgere il tremulo fior,</l>
                  </lg>
                  <lg part="N" org="uniform" sample="complete">
                     <l rend="underline" part="N">
                        <ref xml:id="quot-147-1" target="#comm-096">|</ref> che spiega davanti a Lui solo</l>
                     <l rend="underline" part="N">la pompa del candido velo,</l>
                     <l rend="underline" part="N">che spande ai <ref xml:id="quot-147-2" target="#comm-096a">deserti del cielo</ref>
                     </l>
                     <l part="N">gli olezzi del calice e muor.</l>
                  </lg>
               </section>
            </rdf:value>
