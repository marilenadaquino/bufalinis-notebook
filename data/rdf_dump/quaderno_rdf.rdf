            <rdfs:label>comm</rdfs:label>
         </rdf:Description>
      </oa:motivatedBy>
   </rdf:Description>
   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/comment/comm-048">
      <rdf:type rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
      <oa:hasTarget rdf:resource="https://w3id.org/bufalinis-notebook/citation/t-mann-saggi-su-nietzsche"/>
      <oa:hasBody>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/note/comm-048">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0"
                        type="comment"
                        xml:id="comm-048"
                        corresp="#quot-059"
                        resp="#PB"
                        anchored="true">
                  <list rend="dashed">
                     <item>vita eroica,</item>
                     <item>unita all'adulazione della vita bella e forte
                <list>
                           <item>estetismo eroico</item>
                           <item>estetismo dionisiaco</item>
                        </list>
                     </item>
                  </list>
                  <p part="N">Ciò che lo riconciliava con <persName ref="#SOCR" instant="false" full="yes">Socrate</persName> era la tazza <lb/>di cicuta <lb/>con <persName ref="#GCRI" instant="false" full="yes">Gesù</persName>, la croce.</p>
               </section>
            </rdf:value>
