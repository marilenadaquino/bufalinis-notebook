   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/comment/tra-027">
      <rdf:type rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
      <oa:hasTarget rdf:resource="https://w3id.org/bufalinis-notebook/citation/marziale-2-90"/>
      <oa:hasBody>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/note/tra-027">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0"
                        type="translation"
                        xml:id="tra-027"
                        corresp="#quot-089"
                        resp="#PB"
                        anchored="true">
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">Lieto son io di un focolare, di un tetto che non</l>
                     <l part="N">disdegna il <seg xml:id="t11" part="N">nero</seg>
                        <seg xml:id="t12" part="N">fumo</seg>
                        <listTranspose>
                           <transpose>
                              <ptr target="#t12"/>
                              <ptr target="#t11"/>
                           </transpose>
                        </listTranspose>, di una viva sorgente</l>
                     <l part="N">e di erbe campestri. E <del rend="overstrike" instant="false" status="unremarkable">a me</del> mi <del rend="overstrike" instant="false" status="unremarkable">sia</del>
                        <add place="above" instant="false" status="unremarkable">sian</add> da<del rend="overwrite" instant="false" status="unremarkable">to</del>
                        <add place="superimpose" instant="false" status="unremarkable">ti</add>
                     </l>
                     <l part="N">uno schiavo pasciuto, una moglie non troppo dotta,</l>
                     <l part="N">una notte col sonno, un giorno senza lite.</l>
                  </lg>
               </section>
            </rdf:value>
            <frbr:translationOf rdf:resource="https://w3id.org/bufalinis-notebook/quote/quot-089"/>
            <frbr:realizer rdf:resource="https://w3id.org/bufalinis-notebook/person/pb"/>
