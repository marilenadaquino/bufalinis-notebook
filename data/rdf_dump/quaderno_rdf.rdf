   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/comment/tra-010">
      <rdf:type rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
      <oa:hasTarget rdf:resource="https://w3id.org/bufalinis-notebook/citation/targa-ludovicae-vistarini"/>
