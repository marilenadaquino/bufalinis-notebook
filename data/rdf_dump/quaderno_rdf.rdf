   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/comment/comm-054a">
      <rdf:type rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
      <oa:hasTarget rdf:resource="https://w3id.org/bufalinis-notebook/citation/b-arzeni-mann-schopenhauer-freud"/>
      <oa:hasBody>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/note/comm-054a">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0"
                        xml:id="comm-054a"
                        type="comment"
                        resp="#PB"
                        anchored="true">
                  <p part="N">La concezione hegeliana è ispirata dalla rivoluzione <lb/>cristiana: il Dio che si fa uomo, opera, soffre, promuove <lb/>un mondo diverso, e <del rend="overstrike" instant="false" status="unremarkable">
                        <gap reason="deleted" instant="false"/>
                     </del> dunque non annulla la differenza tra presente <lb/>e passato!!</p>
                  <p part="N">Infatti, l'<persName ref="#BA" instant="false" full="yes">A.</persName> prosegue "Il sostrato dell'opera (di <persName ref="#TM" instant="false" full="yes">Th. Mann</persName>, <lb/>
                     <del rend="overstrike" instant="false" status="unremarkable">Giuseppe</del> Giuseppe, ecc. si serba profondamente aristocratico. Simile <lb/>concezione di dignità e grandezza <add place="above" instant="false" status="unremarkable">
                        <del rend="overstrike" instant="false" status="unremarkable">[sono]</del>[è]</add>
                     <hi rend="underline"> cosa poco <lb/>cristiana</hi>... </p>
               </section>
            </rdf:value>
            <dcterms:references rdf:resource="https://w3id.org/bufalinis-notebook/person/ba"/>
            <dcterms:references rdf:resource="https://w3id.org/bufalinis-notebook/person/tm"/>
