   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/citation/marziale-1-88">
      <rdf:type rdf:resource="http://purl.org/spar/cito/Citation"/>
      <cito:hasCitingEntity rdf:resource="https://w3id.org/bufalinis-notebook/page/62"/>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl088">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">Alcime, quem raptum domino crescentibus annis</l>
                     <l part="N">Lavicana levi cespite velat humus,</l>
                     <l part="N">Accipe non Pario nutantia pondera saxo,</l>
                     <l part="N">Quae cineri vanus dat ruitura labor,</l>
                     <l part="N">Sed faciles buxos et opacas palmitis umbras</l>
                     <l part="N">Quaeque virent lacrimis roscida prata meis</l>
                     <l part="N">Accipe, care puer, nostra monimenta doloris:</l>
                     <l part="N">Hic tibi paerpetuo tempore vivet honor.</l>
                     <l part="N">Cum mihi supremos Lachesis pervenerit annos</l>
                     <l part="N">Non aliter cineres mando iacere meos.</l>
                  </lg>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/62"/>
         </rdf:Description>
      </cito:hasCitedEntity>
