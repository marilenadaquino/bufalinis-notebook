   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/comment/tra-038">
      <rdf:type rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
      <oa:hasTarget rdf:resource="https://w3id.org/bufalinis-notebook/citation/marziale-2-11"/>
      <oa:hasBody>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/note/tra-038">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0"
                        type="translation"
                        xml:id="tra-038"
                        corresp="#quot-099"
                        resp="#PB"
                        anchored="true">
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">Se vedi Selio tutto rannuvolato, o Rufo,</l>
                     <l part="N">che, passeggiando fino ad ora tarda, va consumando</l>
                     <l part="N">il portico; se il suo volto inerte nasconde qualche</l>
                     <l part="N">funebre evento; se il suo enorme naso quasi</l>
                     <l part="N">tocca <del rend="overstrike" instant="false" status="unremarkable">terra</del> la terra; se con la destra si batte</l>
                     <l part="N">il petto e si strappa i capelli:</l>
                     <l part="N">sappi che egli non la morte piange di un amico</l>
                     <l part="N">o di un fratello; e i suoi due figli vivono ed io</l>
                     <l part="N">a loro auguro di vivere; indenni sono la moglie,</l>
                     <l part="N">la roba, i servi, e nessun colono o contadino</l>
                     <l part="N">lo ha fregato. Quale, dunque, la causa di tanto</l>
                     <l part="N">dolore? Cena in casa.</l>
                  </lg>
               </section>
            </rdf:value>
            <frbr:translationOf rdf:resource="https://w3id.org/bufalinis-notebook/quote/quot-099"/>
            <frbr:realizer rdf:resource="https://w3id.org/bufalinis-notebook/person/pb"/>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/66"/>
         </rdf:Description>
      </oa:hasBody>
      <oa:motivatedBy>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/translation">
            <rdfs:label>translation</rdfs:label>
         </rdf:Description>
      </oa:motivatedBy>
      <frbr:translationOf rdf:resource="https://w3id.org/bufalinis-notebook/quote/quot-099"/>
   </rdf:Description>
   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/comment/tra-039">
      <rdf:type rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
      <oa:hasTarget rdf:resource="https://w3id.org/bufalinis-notebook/citation/marziale-2-11"/>
      <oa:hasBody>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/note/tra-039">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0"
                        type="translation"
                        xml:id="tra-039"
                        corresp="#quot-099"
                        resp="#PB"
                        anchored="true">
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">Se vedi Selio con nubilosa fronte <del rend="overstrike" instant="false" status="unremarkable">,</del>
                     </l>
                     <l part="N">passeggiar fino a tardi consumando</l>
                     <l part="N">il portico, Rufo: il <add place="above" instant="false" status="unremarkable">suo</add> volto inerte</l>
                     <l part="N">chiude qualche luttuoso evento;</l>
                     <l part="N">l'enorme naso quasi tocca la terra;</l>
                     <l part="N">con la destra si batte il petto</l>
                     <l part="N">e si strappa i capelli;</l>
                     <l part="N">- non la morte egli piange</l>
                     <l part="N">di un amico o di un fratello;</l>
                     <l part="N">i suoi due figli vivono e io a loro</l>
                     <l part="N">auguro <del rend="overstrike" instant="false" status="unremarkable">di vivere</del>
                        <add place="above" instant="false" status="unremarkable">lunga vita</add>; indenni sono</l>
                     <l part="N">sua moglie, la sua roba, i servi<add place="inline" instant="false" status="unremarkable">;</add>
                     </l>
                     <l part="N">e nessun contadino o colono lo ha fregato.</l>
                     <l part="N">Quale, dunque, <add place="above" instant="false" status="unremarkable">è</add> la causa di <del rend="overstrike" instant="false" status="unremarkable">tanta mestizia</del>
                        <add place="above" instant="false" status="unremarkable">sì grande</add>
                     </l>
                     <l part="N">
                        <del rend="overstrike" instant="false" status="unremarkable">Cena in casa</del>
                        <add place="above" instant="false" status="unremarkable">luttuosa mestizia? Cena in casa.</add>
                     </l>
                  </lg>
               </section>
            </rdf:value>
            <frbr:translationOf rdf:resource="https://w3id.org/bufalinis-notebook/quote/quot-099"/>
            <frbr:realizer rdf:resource="https://w3id.org/bufalinis-notebook/person/pb"/>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/66"/>
         </rdf:Description>
      </oa:hasBody>
      <oa:motivatedBy>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/translation">
            <rdfs:label>translation</rdfs:label>
         </rdf:Description>
      </oa:motivatedBy>
      <frbr:translationOf rdf:resource="https://w3id.org/bufalinis-notebook/quote/quot-099"/>
