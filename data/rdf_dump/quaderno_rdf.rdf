      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl131">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l part="N">Denique sit quodvis, simplex dumtaxat et unum</l>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/115"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl131a">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l part="N">Brevis esse laboro, obscurus fio;</l>
                  <l part="N">sectantem laevia, nervi deficiunt animique;</l>
                  <l part="N">professus grandia, turget;</l>
                  <l part="N">serpit humi tutus nimium timidusque procellae:</l>
                  <l part="N">in vitium ducit culpae fuga, si caret arte</l>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/115"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl131b">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l part="N">Nec facundia deseret hunc, nec lucidus ordo <ref xml:id="quot-156-1" target="#tra-055-1">(1)</ref>
                  </l>
                  <milestone unit="section" rend="short_hrule"/>
                  <l part="N">Ordinis haec virtus erit et venus (aut ego fallor) <ref xml:id="quot-156-2" target="#tra-055-2">(2)</ref>
                  </l>
                  <l part="N">ut iam nunc dicat iam nunc debentia dici</l>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/115"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl131c">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l part="N">notum si callida verbum<ref xml:id="quot-156b-1" target="#tra-055-3">(3)</ref>
                  </l>
                  <l part="N">reddiderit junctura novum.</l>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/115"/>
         </rdf:Description>
      </cito:hasCitedEntity>
   </rdf:Description>
