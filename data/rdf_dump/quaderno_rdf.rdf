      <oa:hasBody>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/note/tra-035">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0"
                        type="translation"
                        xml:id="tra-035"
                        next="#tra-035a"
                        corresp="#quot-098"
                        resp="#PB"
                        anchored="true">
                  <label>(ter)</label>
                  <delSpan spanTo="#d01" instant="false" status="unremarkable"/>
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">Alcimo, tu che nel fior degli anni</l>
                     <l part="N">al tuo signor fosti rapito, ed ora</l>
                     <l part="N">
                        <del rend="overstrike" instant="false" status="unremarkable">ora</del> sulla via Labicana ti copre</l>
                     <l part="N">un velo d'erba leggero,</l>
                     <l part="N">ricevi non un traballante masso</l>
                     <l part="N">di marmo pario, <del rend="overstrike" instant="false" status="unremarkable">a distruzione</del>
                        <add place="above" instant="false" status="unremarkable">ad essere distrutto</add>
                     </l>
                     <l part="N">
                        <del rend="overstrike" instant="false" status="unremarkable">anch'</del> pur esso <del rend="overstrike" instant="false" status="unremarkable">destinato</del>
                        <add place="above" instant="false" status="unremarkable">votato</add>, <del rend="overstrike" instant="false" status="unremarkable">che a poca</del>
                        <add place="above" instant="false" status="unremarkable">offerto</add>
                        <del rend="overstrike" instant="false" status="unremarkable">un mucchio</del>
                        <add place="above" instant="false" status="unremarkable">ad un pugno</add>
                     </l>
                     <l part="N">
                        <del rend="overstrike" instant="false" status="unremarkable">
                           <gap reason="deleted" instant="false"/>
                        </del>di cenere <del rend="overstrike" instant="false" status="unremarkable">offre</del>
                        <add place="above" instant="false" status="unremarkable">da</add> una vana fatica,</l>
                     <l part="N">ma flessibili bossi e ambrosi pampini</l>
                     <l part="N">ricevi, caro fanciullo; e i prati</l>
                     <l part="N">verdeggianti per la rugiada</l>
                     <l part="N">delle mie lacrime: testimonianze</l>
                  </lg>
                  <anchor xml:id="d01"/>
                  <pb n="65" facs="quad035r"/>
                  <delSpan spanTo="#d03" instant="false" status="unremarkable"/>
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">del nostro dolore. In perpetuo</l>
