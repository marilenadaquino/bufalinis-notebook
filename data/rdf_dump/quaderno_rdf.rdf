   <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/citation/t-campanella-madrigale-10">
      <rdf:type rdf:resource="http://purl.org/spar/cito/Citation"/>
      <cito:hasCitingEntity rdf:resource="https://w3id.org/bufalinis-notebook/page/108"/>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl117">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">Desio di desiar tue grazie tengo,</l>
                     <l part="N">certa, evidente vita,</l>
                     <l part="N">quando voglia possente a te m'invita,</l>
                     <l part="N">e, quando è fiacca, avaccio sento il danno;</l>
                     <l part="N">su l'ale del voler non mi sostengo</l>
                     <l part="N">rotte e bagnate</l>
                  </lg>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/108"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="https://w3id.org/bufalinis-notebook/excerpt/bibl060d">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg part="N" org="uniform" sample="complete">
                     <l part="N">E il fuoco, più soffiato, più s'accende,</l>
                     <l part="N">poi vola in alto, e di stelle s'infiora.</l>
                  </lg>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="https://w3id.org/bufalinis-notebook/page/108"/>
         </rdf:Description>
